{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    }
  ],
  "images": {
    "sizes": [640, 750, 828, 1080, 1200],
    "domains": ["cdn.cloudflare.steamstatic.com", "image.api.playstation.com", "helios-i.mashable.com"],
    "minimumCacheTTL": 60
  }
}